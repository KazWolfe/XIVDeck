{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es6",
    "sourceMap": true,
    "resolveJsonModule": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "strict": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": ["./src/**/*"]
}